    "formik": "^2.2.9",
    "gh-pages": "^3.2.3",
    "i18next": "^21.6.10",
    "i18next-browser-languagedetector": "^6.1.3",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-audio-player": "^0.17.0",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.3",
    "react-image-crop": "^9.1.1",
    "react-loader-spinner": "^6.0.0-0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
